# OpenRouter API Configuration
# Multiple API keys (comma-separated) - recommended for better reliability
OPENROUTER_API_KEYS=

# Single API key (legacy support) - leave empty when using multiple keys

# Model to use (specify the exact model name from OpenRouter)
# Other examples: meta-llama/llama-3.1-8b-instruct:free, anthropic/claude-3-haiku
OPENROUTER_MODEL=x-ai/grok-4.1-fast:free

# Enable reasoning for supported models (like Grok)
OPENROUTER_ENABLE_REASONING=true

OPENROUTER_SITE_NAME=Document Query System

# Database Configuration
DATABASE_TYPE=supabase
MAX_STORAGE_SIZE=1073741824

# Supabase Configuration (if using Supabase)
SUPABASE_URL=
SUPABASE_KEY=



# MCP Server Configuration
MCP_ENABLED=true

# Obsidian REST API Configuration
OBSIDIAN_API_URL=
OBSIDIAN_API_KEY= 
OBSIDIAN_VAULT_PATH=

# Application Configuration
DEBUG=true
LOG_LEVEL=INFO
UPLOAD_DIR=uploads

